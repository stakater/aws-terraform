ELASTICSEARCH_IMAGE=elasticsearch:latest
ELASTICSEARCH_OPTS="-p 9200:9200 -p 9300:9300"

LOGSTASH_IMAGE=logstash:latest
LOGSTASH_OPTS="-p 5000:5000 -v /var/lib/apps/elk/config:/var/lib/logstash/config"

KIBANA_IMAGE=kibana:latest
KIBANA_OPTS="--link elasticsearch.service:elasticsearch -p 5601:5601"
KIBANA_PUBLIC_PORT=5601
